{
  "name": "tmaui",
  "version": "2.1.8",
  "description": "World-class, ultimate UI developer toolkit.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "./dist"
  ],
  "sideEffects": [
    "./dist/index.js",
    "./dist/cjs/index.js"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "turbo run tsc:es6 tsc:cjs swc:es6 swc:cjs postcss",
    "build:types": "turbo run tsc:es6 tsc:cjs",
    "build:js-css": "turbo run swc:es6 swc:cjs postcss",
    "postcss": "webpack --config webpack.styles.config.js",
    "swc-base": "cross-env NODE_ENV=production swc src/ --config-file package.swcrc --extensions .tsx,.jsx,.ts,.js --strip-leading-paths",
    "swc:es6": "npm run swc-base -- -d dist -s",
    "swc:cjs": "npm run swc-base -- -d dist/cjs -s -C module.type=commonjs",
    "tsc:es6": "cross-env NODE_ENV=production tsc --emitDeclarationOnly --declaration -p tsconfig.dist.json && tsc-alias",
    "tsc:cjs": "cross-env NODE_ENV=production tsc --emitDeclarationOnly --declaration --outDir dist/cjs/ -p tsconfig.dist.json && tsc-alias",
    "check": "turbo run test lint typecheck",
    "test": "jest",
    "lint": "eslint ./src --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "dev": "turbo run storybook css:types:dev --cache=local:r,remote:r",
    "storybook": "storybook dev -p 6006 --docs --no-open",
    "storybook:build": "storybook build --docs",
    "css:types": "tcm -p 'src/**/*.module.*css'",
    "css:types:dev": "tcm -p 'src/**/*.module.*css' --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tophackr/tmaui.git"
  },
  "author": "Alexandr Musikhin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tophackr/tmaui/issues"
  },
  "homepage": "https://github.com/tophackr/tmaui#readme",
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.50.0",
    "@eslint/js": "^9.27.0",
    "@floating-ui/core": "^1.7.0",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addons": "^7.6.10",
    "@storybook/react": "^7.6.7",
    "@storybook/react-webpack5": "^7.6.7",
    "@svgr/webpack": "^8.1.0",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.11.29",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.15.27",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.7",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import-x": "^4.14.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-commented-code": "^1.0.10",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^59.0.1",
    "file-loader": "^6.2.0",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "jiti": "^2.4.2",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.4",
    "postcss-gap-properties": "^6.0.0",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-logical": "^8.1.0",
    "postcss-modules": "^6.0.1",
    "prettier": "^3.1.1",
    "remove-files-webpack-plugin": "^1.5.0",
    "storybook": "^7.6.7",
    "swc-loader": "^0.2.6",
    "swc-plugin-css-modules": "^3.1.1",
    "swc-plugin-transform-remove-imports": "^4.0.4",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "tsc-alias": "^1.8.8",
    "turbo": "^2.5.3",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.0.8",
    "@floating-ui/utils": "^0.2.9",
    "@swc/helpers": "^0.5.17",
    "@twa-dev/types": "^7.0.0",
    "@xelene/vaul-with-scroll-fix": "0.1.4"
  },
  "resolutions": {
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5"
  },
  "packageManager": "pnpm@10.8.0"
}
